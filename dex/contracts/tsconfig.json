{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "types": [
      "node",
      "hardhat"
    ]
  },
  "include": [
    "./hardhat.config.cts",
    "./scripts"
  ]
}
